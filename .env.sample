# SiteBuilder Chat Environment Variables
# Copy this file to .env and configure your settings

# Required: n8n Cloud webhook URL for chat communication
# Example: https://hook.n8n.cloud/webhook/sitebuilder-chat
VITE_WEBHOOK_URL=

# Optional: Bearer token for webhook authentication
# Leave empty if no authentication is required
VITE_WEBHOOK_TOKEN=

# Optional: Chat widget title (defaults to "SiteBuilder")
VITE_CHAT_TITLE=SiteBuilder

# Optional: Enable debug mode for development (true/false)
VITE_DEBUG=false
